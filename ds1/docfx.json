{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "removeHostName": false,    
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/ds1/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "no-loc": ["update"],
      "layout": "Conceptual"
    },
    "fileMetadata": {
      "no-loc": {
        "**/*.md": ["dummy"]
      }
    },
    "template": [],
    "dest": "ds1",
    "updateTimeAsCommitBuildTime": true,
    "updateCommitBuildTime": true,
    "documentIdOverride": "document-ids.json"
  }
}